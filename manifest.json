{
  "manifest_version": 3,
  "name": "Get Schedule",
  "version": "2.0",
  "description": "Get Exam Routine",
  "permissions": ["storage", "tabs", "activeTab"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "32": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["xlsx.full.min.js", "content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["*.xlsx"],
      "matches": ["<all_urls>"]
    }
  ]
}
